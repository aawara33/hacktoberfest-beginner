// Non Empty array example 
// CPP program to illustrate 
// Implementation of empty() function 
#include <array> 
#include <iostream> 
using namespace std; 

int main() 
{ 
	array<int, 5> myarray{ 1, 2, 3, 4 }; 
	if (myarray.empty()) { 
		cout << "True"; 
	} 
	else { 
		cout << "False"; 
	} 
	return 0; 
}
